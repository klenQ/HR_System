t type=\"text\" class=\"form-control\" placeholder=\"请输入编号\" v-model=\"form.number\">\r\n                </div>\r\n            </div>\r\n            <div class=\"form-group\">\r\n                <label class=\"col-sm-2 control-label\">名字</label>\r\n                <div class=\"col-sm-10\">\r\n                    <input type=\"text\" class=\"form-control\" placeholder=\"请输入名字\"\r\n                           v-model=\"form.name\">\r\n                </div>\r\n            </div>\r\n            <div class=\"form-group\">\r\n                <label class=\"col-sm-2 control-label\">性别</label>\r\n                <div class=\"col-sm-10\">\r\n                    <input type=\"radio\" value=\"男\" v-model=\"form.gender\"/>男 <input\r\n                        type=\"radio\" value=\"女\" v-model=\"form.gender\"/>女\r\n                </div>\r\n            </div>\r\n            <div class=\"form-group\">\r\n                <label class=\"col-sm-2 control-label\">年龄</label>\r\n                <div class=\"col-sm-10\">\r\n                    <input type=\"text\" class=\"form-control\" placeholder=\"请输入年龄\"\r\n                           v-model=\"form.age\">\r\n                </div>\r\n            </div>\r\n            <div class=\"form-group\">\r\n                <label class=\"col-sm-2 control-label\">部门</label>\r\n                <div class=\"col-sm-10\">\r\n                    <select class=\"form-control\" v-model=\"form.dep.id\">\r\n                        <option v-for=\"dep in depList\"\r\n                                v-text=\"dep.name\" v-bind:key=\"dep.id\" v-bind:value=\"dep.id\">\r\n                        </option>\r\n                    </select>\r\n                </div>\r\n            </div>\r\n            <div class=\"form-group\">\r\n                <div class=\"col-sm-offset-2 col-sm-10\">\r\n                    <button type=\"submit\" class=\"btn btn-primary\">保存</button>\r\n                </div>\r\n            </div>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"Update\",\r\n        data() {\r\n            return {\r\n                form: {\r\n                    id: null,\r\n                    number: null,\r\n                    name: null,\r\n                    gender: null,\r\n                    age: null,\r\n                    dep: {\r\n                        id: null\r\n                    }\r\n                },\r\n                depList: []\r\n            }\r\n\r\n        },\r\n        methods: {\r\n            update: function () {\r\n                this.axios.post('/emp', this.form)\r\n                    .then(function (res) {\r\n                        if (res.data == true) {\r\n                            this.$router.push({name: \"EmpShow\"})\r\n                        } else {\r\n                            alert(\"系统错误\");\r\n                        }                    }.bind(this))\r\n            },\r\n            searchById: function () {\r\n                this.axios.get('/emp/'+ this.form.id)\r\n                    .then(function (res) {\r\n                        this.form = res.data;\r\n                        if (this.form.dep == null) {\r\n                            this.form.dep = {id: this.depList[0].id};\r\n                        }\r\n                    }.bind(this))\r\n            },\r\n            searchDep: function () {\r\n                this.axios.get('/dep')\r\n                    .then(function (res) {\r\n                        this.depList = res.data;\r\n\r\n                    }.bind(this))\r\n            },\r\n\r\n        },\r\n        created() {\r\n            this.form.id = this.$route.params.id;\r\n            this.searchDep();\r\n            this.searchById();\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    #container {\r\n        width: 600px;\r\n        margin: 10px auto;\r\n    }\r\n</style>"]}]}